<?xml version="1.0" encoding="utf-8"?>
<jamp>
	<page typeobj="page" title="JApp - Cambio Password" loaddata="true">
		<xmlpage typeobj="xmlpage" src="intestazione.xml" />
		<ds1 typeobj="ds" conn="conn6" dstable="es2_account" dskey="key" event="none" dswhere="`key`='-1'" encpwd="md5"/>
		<div2 typeobj="div" style="text-align: center;">
			<label1 typeobj="label" value="Cambio Password:" style="font-weight: bold; font-size: 2em;"/>
			<user typeobj="hidden" value="$$LOGIN-USER$$" dsitem="username"/>
			<oldpwd typeobj="text" label="\nVecchia Password:\n" maxlength="20" size="20" password="true" minlength="1" dsitem="password"/>
			<pwd1 typeobj="text" label="\nNuova Password:\n" maxlength="20" size="20" password="true" minlength="8" dsitem="password"/>
			<pwd2 typeobj="text" label="\nRipeti Password:\n" maxlength="20" size="20" password="true" dsitem="password"/>
			<label2 typeobj="button" label="\n\n" value="Cambia" onclick="AJAX.changePassword('ds1','user','pwd1','oldpwd')" style="font-weight: bold; font-size: 1.2em; cursor: pointer;"/>
		</div2>
	</page>
</jamp>
