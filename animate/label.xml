<?xml version="1.0" encoding="utf-8"?>
<jamp>
	<page id="page" title="JAMP - Effetto Animazione Label" loaddata="true" icon="../../favicon.ico" out="html">
		<fieldset id="frame1" legend="Pannello di Controllo">
			<dsselect id="select1" onchange="getCSS1();" label="Proprietà: " />
			<text id="text1" label=":" />
			<dsselect id="select2" label="Algoritmo: " optionvalue="default,sin,cos" />
			<text id="text2" size="5" label="Durata ms:" value="1000" keypress="number" />
			<text id="text3" size="3" label="fps:" value="25" keypress="number" onchange="ANIMATE.fps = this.value;" />
			<button id="button1" onclick="startanimate();" value="START" />
			<dsselect id="select1_1" onchange="getCSS2();" label="\nProprietà: " />
			<text id="text1_1" label=":" />
			<label id="label1" value="-" label="\nCodice JS:\n" />
		</fieldset>
		<label id="obj1" value="Testo Animazione!" style="position:absolute;" />
	</page>
</jamp>
