<?xml version="1.0" encoding="utf-8"?>
<jamp>
	<page typeobj="page" title="JAMP - Esempio Animazione Autenticazione" icon="../../favicon.ico" loaddata="true">
		<ds1 typeobj="ds" conn="conn6" dstable="utenti" dskey="key" event="none" dswhere="`key`='-1'" encpwd="md5" />
			<div1 typeobj="div" class="login">
				<user typeobj="text" label="\nUtente:\n" maxlength="20" size="20" dsitem="user" onkeypress="REGEXP.nextFocus(event, 'pwd', 13);" />
				<pwd typeobj="text" label="\nPassword:\n" maxlength="20" size="20" password="true" dsitem="pwd" onkeypress="REGEXP.actionClick(event, 'button1', 13);" />
				<button1 typeobj="button" label="\n" value="Accedi" onclick="AJAX.login('ds1', 'user', 'pwd');" />
		</div1>
	</page>
</jamp>
