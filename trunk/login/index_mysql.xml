<?xml version="1.0" encoding="utf-8"?>
<jamp>
	<page id="page" title="JAMP - Esempio di Autenticazione" icon="../../favicon.ico" loaddata="true">
		<ds id="ds1" conn="conn6" dstable="utenti" dskey="key" event="none" load="false" encpwd="md5" />
		<div id="div1" style="height : 150px; top : 50%; position : absolute; width:99%; margin-top : -50px;">
			<table id="table1" align="center">
				<tr id="tr1">
					<td id="td2">
						<label id="label1" value="JAMP:" style="font-weight: bold; font-size: 2em;" />
						<text id="user" label="\nUtente(demo):\n" maxlength="20" size="20" dsitem="user" onkeypress="REGEXP.nextFocus(event, 'pwd', 13);" />
						<text id="pwd" label="\nPassword(demo):\n" maxlength="20" size="20" password="true" dsitem="pwd" onkeypress="REGEXP.actionClick(event, 'button1', 13);" />
						<button id="button1" label="\n\n" value="Accedi" onclick="AJAX.login('ds1', 'user', 'pwd');" style="font-weight: bold; font-size: 1.2em; cursor: pointer;" />
					</td>
				</tr>
			</table>
		</div>
	</page>
</jamp>
