<?xml version="1.0" encoding="utf-8"?>
<jamp>
	<page typeobj="page" title="JAMP - Dynamic Graph" out="html" icon="../../favicon.ico" loaddata="true">
		<ds1 typeobj="ds" conn="conn6" dstable="graph" dskey="id" />
		<div1 typeobj="div" style="float: left">	
			<graphic1 typeobj="graphic" width="500" height="400" scale="textlin">
				<parameter>
					<plot objtype="plot" shadow="true" frame="false" plotwidth="0.4" />
					<title objtype="title" title="Dinamic Graph" font="FF_FONT1,FS_BOLD" margin="3" />
					<xaxis1 objtype="xaxis" title="asse x" font="FF_FONT1" dsobj="ds1" dsitem="xaxis" />
					<legend1 objtype="legend" pos="0.02,0.15" shadow="darkgray@0.5" fillcolor="lightblue@0.3" />				
					<bar1 objtype="bar" dsobj="ds1" dsitem="bar1" fillcolor="orange" shadow="orange@0.4" />
				</parameter>
			</graphic1>
		</div1>
		<div2 typeobj="div" style="margin: 10px">	
			<dsnav1 typeobj="dsnav" dsobj="ds1" btNew="false" btPage="false" btNav="false" btDelete="false" btCancel="false" btPrint="false" />
			<gridds1 typeobj="gridds" dsobj="ds1" width="300" height="400">
				<coloumn>   
					<col objtype="text" itemlabel="Asse X" dsitem="xaxis" minlength="3" maxlength="3" size="5" />
					<col objtype="text" itemlabel="Valori Y" dsitem="bar1" keypress="number" minlength="1" maxlength="2" size="5" />
				</coloumn>
			</gridds1>
		</div2>
	</page>
</jamp>