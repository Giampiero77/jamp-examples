<?xml version="1.0" encoding="utf-8"?>
<jamp>
	<page typeobj="page" title="JAMP - Esempio di Autenticazione" icon="../../favicon.ico" loaddata="true">
		<ds1 typeobj="ds" conn="conn3" dskey="dn" scope="base" event="none" base="ou=Users,dc=jamp,dc=it" loadall="false" />
		<div1 typeobj="div" style="height : 150px; top : 50%; position : absolute; width:99%; margin-top : -50px;">
			<table1 typeobj="table" align="center">
				<tr1 typeobj="tr">
					<td2 typeobj="td">
						<label1 typeobj="label" value="JAMP:" style="font-weight: bold; font-size: 2em;"/>
						<user typeobj="text" label="\nUtente:\n" maxlength="20" size="20" dsitem="user" onkeypress="REGEXP.nextFocus(event, 'pwd', 13);" />
						<pwd typeobj="text" label="\nPassword:\n" maxlength="20" size="20" password="true" dsitem="pwd" onkeypress="REGEXP.actionClick(event, 'button1', 13);" />
						<button1 typeobj="button" label="\n\n" value="Accedi" onclick="AJAX.login('ds1', 'user', 'pwd');" style="font-weight: bold; font-size: 1.2em; cursor: pointer;"/>
					</td2>
				</tr1>
			</table1>
		</div1>
	</page>
</jamp>