<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE jamp PUBLIC "jamp" "http://jamp.alyx.it/jamp.dtd">
<jamp>
	<page id="page" title="JAMP - Esempio della gridds collegata a un filesystem" icon="../../favicon.ico" loaddata="true">
		<ds id="ds1" conn="conn1" dskey="path" scope="onelevel" base="../.." dslimit="10" />
		<dsnav id="dsnav1" dsobj="ds1" btNew="false" objprint="gridds1" />
		<gridds id="gridds1" dsobj="ds1" width="autosize" height="autosize">
			<coloumn>
				<label itemlabel="Path" dsitem="path" order="false" colwidth="250px" />
				<text itemlabel="Nome" dsitem="filename" order="false" colwidth="100px" />
				<label itemlabel="Tipo" dsitem="type" order="false" colwidth="100px" />
			</coloumn>
		</gridds>
	</page>
</jamp>
